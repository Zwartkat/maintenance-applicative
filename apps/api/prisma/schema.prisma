generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Users {
  id       Int       @id @default(autoincrement())
  email String       @unique
  password String
}

model Professors {
  id           Int     @id @default(autoincrement())
  firstName    String
  lastName     String
  email        String  @unique
  upVotes      Int     @default(0)
  downVotes    Int     @default(0)
  officeNumber Int?
}